{
    "rewrites": [
        {
            "source": "/api/(.*)",
            "destination": "/api/main.py"
        },
        {
            "source": "/webhook/shopify",
            "destination": "/api/main.py"
        },
        {
            "source": "/stats",
            "destination": "/api/main.py"
        },
        {
            "source": "/orders(.*)",
            "destination": "/api/main.py"
        },
        {
            "source": "/health",
            "destination": "/api/main.py"
        }
    ]
}